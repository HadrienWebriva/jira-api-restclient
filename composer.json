{
    "name": "chobie/jira-api-restclient",
    "description": "JIRA REST API Client",
    "keywords": ["jira","rest","api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shuhei Tanuma",
            "homepage": "http://chobie.github.io/",
            "email": "chobieeee@php.net"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "aik099/coding-standard": "dev-master",
        "squizlabs/php_codesniffer": "3.*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4" : {
            "chobie\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\chobie\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
	"config": {
		"platform": {
			"php": "7.4"
		}
	}
}
